{
  "name": "@teleporthq/teleport-generator-react",
  "version": "0.0.8",
  "description": "React code generator for teleportHQ",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "link-dev": "npm link ../teleport-lib-js",
    "clean": "rm -rf dist",
    "lint": "tslint --project ./tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage --no-cache",
    "precommit": "pretty-quick --staged && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teleporthq/teleport-generator-react.git"
  },
  "author": "teleportHQ",
  "license": "MIT",
  "homepage": "https://github.com/teleporthq/teleport-generator-react#README",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "codecov": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "prettier": "^1.13.6",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@teleporthq/teleport-lib-js": "0.0.4",
    "@types/node": "^9.6.0",
    "lodash": "^4.17.10",
    "prettier-standalone": "^1.3.1-0"
  }
}
